include "hex.ts4"

fn main(--)
	' ' emit
	'x' emit
	0u8 0x10u8 for i do
		i 0x07u8 and 0u8 == if
			' ' emit
		end
		i printNibble
	end
	'\n' emit

	0x20u8 0x80u8 for i do
		i 0x0Fu8 and 0u8 == if
			i printHex8
		end
		i 0x07u8 and 0u8 == if
			' ' emit
		end
		i emit
		i 0x0Fu8 and 0x0Fu8 == if
			'\n' emit
		end
	end
end
